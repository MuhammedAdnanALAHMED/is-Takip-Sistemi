        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Adres)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Durum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sifre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Gorsel)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.ID })
            @Html.ActionLink("Delete", "Delete", new { id = item.ID })

        </td>
    </tr>
}

</table>
