@model IEnumerable<MvcFirmaCagri.Models.Entity.TblFirmalar>

@{
    ViewBag.Title = "progiter";
    Layout = "~/Views/Shared/FirmaLayout.cshtml";
}

<h2>profil Düzeltme</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ad)
